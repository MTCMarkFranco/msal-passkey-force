{
  "name": "secure-kiosk-app",
  "version": "1.0.0",
  "description": "Secure Kiosk React App with Entra ID, PKCE, and Passkey Authentication",
  "main": "server/index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "webpack serve --mode development",
    "build": "webpack --mode production && npm run server:build",
    "server:build": "echo 'Server build complete'",
    "start": "node server/index.js",
    "client:build": "webpack --mode production"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@azure/msal-browser": "^3.5.0",
    "@azure/msal-node": "^2.5.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.1",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "react",
    "entra-id",
    "msal",
    "pkce",
    "passkey",
    "kiosk",
    "security"
  ],
  "author": "",
  "license": "MIT"
}
{
  "name": "secure-kiosk-app",
  "version": "1.0.0",
  "description": "Secure Kiosk React App with Entra ID, PKCE, and Passkey Authentication",
  "main": "server/index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "npm run build && npm run server:dev",
    "dev:watch": "concurrently \"npm run client:watch\" \"npm run server:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "webpack serve --mode development",
    "client:watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "start": "npm run build && node server/index.js",
    "start:prod": "node server/index.js"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/msal-browser": "^3.5.0",
    "@azure/msal-node": "^2.16.3",
    "@simplewebauthn/browser": "^13.2.0",
    "@simplewebauthn/server": "^13.2.1",
    "axios": "^1.6.0",
    "base64url": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^3.0.2",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "keywords": [
    "react",
    "entra-id",
    "msal",
    "pkce",
    "passkey",
    "kiosk",
    "security"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MTCMarkFranco/msal-passkey-force.git"
  },
  "author": "MTCMarkFranco",
  "license": "MIT"
}
